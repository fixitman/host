{
    "Server": {
        "URL": "http://fixitmanmike2.ddns.net",
        "Port": 80
    },

    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft":"Warning",
                "System":"Warning"
            }
        },
        "WriteTo": [
            { "Name": "Console" },
            { "Name": "File", 
                "Args": { 
                    "path": "Logs/log.txt",
                    "rollingInterval": 2
                }
             }
        ],
        "Properties": {
            "Application": "Sample"
        }
    }
}